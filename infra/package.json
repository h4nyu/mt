{
  "name": "@kgy/infra",
  "version": "0.1.0",
  "main": "index.js",
  "license": "MIT",
  "types": "index.ts",
  "scripts": {
    "clean": "tsc --build --clean",
    "build": "yarn generate && tsc --noEmit false",
    "generate": "pnpify prisma generate",
    "prepack": "yarn build",
    "postpack": "yarn clean",
    "test": "yarn node --experimental-vm-modules $(yarn bin jest)"
  },
  "dependencies": {
    "@google-cloud/storage": "^6.12.0",
    "@kgy/core": "0.1.0",
    "@prisma/client": "5.0.0",
    "@yarnpkg/pnpify": "^4.0.0-rc.48",
    "axios": "^0.26.1",
    "date-fns": "^2.28.0",
    "fastify": "^4.9.2",
    "lodash": "^4.17.21",
    "nanoid": "3.3.1",
    "postgres": "^2.0.0-beta.11",
    "prisma": "^5.0.0",
    "winston": "^3.10.0",
    "ws": "^8.9.0"
  },
  "jest": {
    "preset": "ts-jest/presets/js-with-babel-esm",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "testEnvironment": "node",
    "testRegex": ".*\\.(test|spec)?\\.(ts|tsx)$"
  },
  "devDependencies": {
    "@types/jest": "^27.5.1",
    "@types/node": "^17.0.35",
    "@types/ws": "^8.5.3",
    "jest": "^28.1.0",
    "ts-jest": "^28.0.2",
    "typescript": "^4.6.4"
  }
}
